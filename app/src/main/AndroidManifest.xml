<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.contactlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.contactlist.MainActivity5"></activity>
        <activity android:name="com.example.contactlist.MainActivity4" />
        <activity android:name="com.example.contactlist.MainActivity3" />
        <activity android:name="com.example.contactlist.MainActivity2" />
        <activity android:name="com.example.contactlist.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.contactlist.addNewContact"
            android:label="Add new contact" />
        <activity
            android:name="com.example.contactlist.DetailsContact"
            android:label="Contact Details" />
        <activity
            android:name="com.example.contactlist.EditContact"
            android:label="Edit Contact" />
    </application>

</manifest>